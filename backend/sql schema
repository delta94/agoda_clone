CREATE TABLE IF NOT EXISTS `users` (`id` INTEGER NOT NULL auto_increment , `email` VARCHAR(255) NOT NULL, `password` VARCHAR(255), `first_name` VARCHAR(255), `last_name` VARCHAR(255), `createdAt` DATETIME NOT NULL, `updatedAt` DATETIME NOT NULL, PRIMARY KEY (`id`)) ENGINE=InnoDB;



CREATE TABLE IF NOT EXISTS `mobiles` (`id` INTEGER NOT NULL auto_increment , `number` INTEGER NOT NULL, `password` VARCHAR(255), `first_name` VARCHAR(255), `last_name` VARCHAR(255), `createdAt` DATETIME NOT NULL, `updatedAt` DATETIME NOT NULL, PRIMARY KEY (`id`)) ENGINE=InnoDB;


CREATE TABLE IF NOT EXISTS `properties` (`id` INTEGER NOT NULL auto_increment , `name` VARCHAR(255) NOT NULL, `type` VARCHAR(255), `address` VARCHAR(255), `available` TINYINT(1), `date` DATETIME, `city` VARCHAR(255), `country` VARCHAR(255), `createdAt` DATETIME NOT NULL, `updatedAt` DATETIME NOT NULL, PRIMARY KEY (`id`)) ENGINE=InnoDB;

CREATE TABLE IF NOT EXISTS `rooms` (`id` INTEGER NOT NULL auto_increment , `name` VARCHAR(255) NOT NULL, `amount` INTEGER, `wifi` TINYINT(1), `creditCard` TINYINT(1), `cashonDelivery` TINYINT(1), `cancellation` TINYINT(1), `booked` TINYINT(1), `reserved` TINYINT(1), `housekeep` TINYINT(1), 
`breakdast` TINYINT(1), `size` INTEGER, `rating` INTEGER, `desc` VARCHAR(255), `date` DATETIME, `createdAt` DATETIME NOT NULL, `updatedAt` DATETIME NOT NULL, 
`propId` INTEGER, PRIMARY KEY (`id`), FOREIGN KEY (`propId`) REFERENCES `properties` (`id`) ON DELETE SET NULL ON UPDATE CASCADE) ENGINE=InnoDB;


CREATE TABLE IF NOT EXISTS `views` (`id` INTEGER NOT NULL auto_increment , `desc` VARCHAR(255) NOT NULL, `rating` VARCHAR(255), `topic` VARCHAR(255), `available` TINYINT(1), `date` DATETIME, `userId` INTEGER NOT NULL, `createdAt` DATETIME NOT NULL, `updatedAt` DATETIME NOT NULL, `roomId` INTEGER, PRIMARY KEY (`id`), FOREIGN KEY (`roomId`) REFERENCES `rooms` (`id`) ON DELETE SET NULL ON UPDATE CASCADE) ENGINE=InnoDB;
